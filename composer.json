{
    "name": "allenjb/sql",
    "type": "library",
    "description": "Aura.Sql / Aura.SqlQuery extensions",
    "keywords": [
        "AllenJB",
        "MySQL",
        "database"
    ],
    "homepage": "https://github.com/AllenJB/Sql",
    "license": "MIT",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-pdo": "*",
        "aura/sql": "^4.0|^5.0",
        "aura/sqlquery": "^2.7",
        "aura/sqlschema": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AllenJB\\Sql\\": "src",
            "AllenJB\\SqlQuery\\": "src\\Query"
        }
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-strict-rules": "^0.12.11",
        "phpunit/phpunit": "^9.5",
        "slam/phpstan-extensions": "^5.0.2"
    }
}
